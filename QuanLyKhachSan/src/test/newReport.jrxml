	</parameter>
	<queryString>
		<![CDATA[SELECT
     ChiTietHoaDon."MaChiTietHoaDon" AS ChiTietHoaDon_MaChiTietHoaDon,
     ChiTietHoaDon."MaPhong" AS ChiTietHoaDon_MaPhong,
     ChiTietHoaDon."MaHoaDon" AS ChiTietHoaDon_MaHoaDon,
     KhachHang."TenKhachHang" AS KhachHang_TenKhachHang,
     Phong."SoPhong" AS Phong_SoPhong
FROM
     "dbo"."KhachHang" KhachHang INNER JOIN "dbo"."HoaDon" HoaDon ON KhachHang."SoCMTKhachHang" = HoaDon."SoCMTKhachHang"
     INNER JOIN "dbo"."ChiTietHoaDon" ChiTietHoaDon ON HoaDon."MaHoaDon" = ChiTietHoaDon."MaHoaDon"
     INNER JOIN "dbo"."Phong" Phong ON ChiTietHoaDon."MaPhong" = Phong."MaPhong"
WHERE
     SoPhong = $P{SoPhong}]]>
	</queryString>
